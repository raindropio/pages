{
  "name": "pages",
  "version": "1.0.0",
  "license": "MIT",
  "targets": {
    "client": {
      "source": "src/client/index.html",
      "distDir": "public",
      "context": "browser"
    },
    "worker": {
      "source": [
        "src/worker/[[ssr]].jsx"
      ],
      "distDir": "functions",
      "outputFormat": "esmodule",
      "isLibrary": true
    }
  },
  "scripts": {
    "local": "parcel",
    "local:prod": "parcel build && yarn wrangler pages dev ./public"
  },
  "devDependencies": {
    "parcel": "^2.0.1",
    "parcel-plugin-nuke-dist": "^1.0.1",
    "wrangler": "^0.0.0-fsw-beta.5"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "6",
    "swr": "^1.0.1"
  }
}
