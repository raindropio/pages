{
	"name": "pages",
	"version": "0.0.0",
	"imports": {
		"~*": "./src/*.js"
	},
	"scripts": {
		"local": "node ./etc/dev-server",
		"preview": "yarn build:vite && sudo wrangler dev --port 80",
		"deploy:prod": "yarn build:vite && wrangler publish",
		"build": "yarn build:vite && yarn build:worker",
		"build:vite": "vite build && vite build --ssr",
		"build:worker": "wrangler build"
	},
	"dependencies": {
		"@cloudflare/kv-asset-handler": "~0.1.2",
		"@cloudflare/wrangler": "^1.16.1",
		"@vitejs/plugin-react": "^1.0.4",
		"express": "^4.17.1",
		"node-fetch": "^2.6.1",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-helmet": "^6.1.0",
		"vite": "2.5.10",
		"vite-plugin-ssr": "0.3.31"
	},
	"devDependencies": {
		"eslint": "^7.23.0",
		"eslint-plugin-react": "^7.23.1",
		"eslint-plugin-react-hooks": "^4.2.0"
	}
}
